    <!--Questions -->
    <string name="question1">What is the maximum amount of time that should be spent in one location?</string>
    <string name="question2">I am the kind of wild camper that&#8230;</string>
    <string name="question3">The best way to dispose of human waste is&#8230;</string>
    <string name="question4">Deadwood should be&#8230;</string>
    <string name="question5">It is acceptable to leave food scraps as&#8230;</string>
    <string name="question6">I am the kind of wild camper who&#8230;</string>
    <string name="question7">It is acceptable to move boulders so long as&#8230;</string>

    <!--Answers -->
    <string name="question1_answer1">1 night</string>
    <string name="question1_answer2">2 or 3 nights</string>
    <string name="question1_answer3">3 to 5 nights</string>
    <string name="question1_answer4">There is no maximum</string>
    <string name="question2_answer1">Leaves no trace</string>
    <string name="question2_answer2">Leaves some trace</string>
    <string name="question2_answer3">Leaves a lot of trace</string>
    <string name="question2_answer4">Leaves as much trace as possible</string>
    <string name="question3_answer1">in a stream, river or loch.</string>
    <string name="question3_answer2">on the ground and cover with tissue paper.</string>
    <string name="question3_answer3">carry out in biodegradable bag.</string>
    <string name="question3_answer4">bury underground.</string>
    <string name="question4_answer1">burned on a camp-fire, it is free fuel.</string>
    <string name="question4_answer2">taken with you, it is free wood.</string>
    <string name="question4_answer3">left alone, it is an important habitat for insects.</string>
    <string name="question4_answer4">moved out of the way so no one can trip over it.</string>
    <string name="question5_answer1">birds will eat them.</string>
    <string name="question5_answer2">they will degrade naturally over time.</string>
    <string name="question5_answer3">it is not acceptable to leave food scraps, but fruit peelings are OK.</string>
    <string name="question5_answer4">it is not acceptable to leave any food scraps.</string>
    <string name="question6_answer1">removes all of my own litter.</string>
    <string name="question6_answer2">leaves some litter for next person.</string>
    <string name="question6_answer3">leaves some litter for the animals.</string>
    <string name="question6_answer4">removes all of my own and others\' litter.</string>
    <string name="question7_answer1">it is not covered in moss.</string>
    <string name="question7_answer2">you move it back to where you found it.</string>
    <string name="question7_answer3">it won\'t roll away.</string>
    <string name="question7_answer4">there are lots of boulders.</string>

